{
  "name": "@e-life/monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": "gitlab:xcoins-tech/xcoins-core-services/monorepo",
  "scripts": {
    "prepare": "is-ci || husky install",
    "start": "docker-compose up -d --build --remove-orphans",
    "stop": "docker-compose down",
    "lint": "nx run-many --target=lint",
    "lint:fix": "pnpm -r lint:fix",
    "build": "nx run-many --target=build",
    "start:dev": "nx run-many --target=start:dev",
    "test": "nx run-many --target=test --bail",
    "test:coverage": "pnpm test -- -- --coverage",
    "document": "nx run-many --target=document",
    "ci:coverage-report": "node --loader ts-node/esm scripts/ci-coverage-report.mts",
    "env:copy": "pnpm -r env:copy",
    "migrate": "pnpm -r migrate",
    "seed": "pnpm -r seed",
    "migrate:seed": "pnpm -r migrate:seed",
    "migrate:rollback": "pnpm -r migrate:rollback",
    "migrate:rollback:all": "pnpm -r migrate:rollback:all",
    "migrate:fresh": "pnpm -r migrate:fresh",
    "migrate:fresh:seed": "pnpm -r migrate:fresh:seed",
    "init": "node ./packages/shared-libraries/scripts.js init",
    "start:us": "pnpm --filter @e-life/auth-service start:dev",
    "start:cs": "pnpm --filter @e-life/communication-service start:dev",
    "start:ms": "pnpm --filter @e-life/message-service start:dev",
    "start:me": "pnpm --filter @e-life/me-service start:dev",
    "start:ns": "pnpm --filter @e-life/notification-service start:dev",
    "start:pays": "pnpm --filter @e-life/payment-service start:dev",
    "start:ps": "pnpm --filter @e-life/property-service start:dev",
    "start:rrs": "pnpm --filter @e-life/review-rating-service start:dev",
    "test:cypress:rate": "npx cypress open --project ./apps/rate-service"
  },
  "engines": {
    "node": ">= 16.13.2",
    "pnpm": ">= 9.0.1"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@compodoc/compodoc": "1.1.21",
    "@golevelup/ts-jest": "^0.4.0",
    "@jest/types": "29.4.3",
    "@nestjs/cli": "9.1.8",
    "@nrwl/jest": "15.8.5",
    "@nrwl/js": "15.8.5",
    "@nrwl/linter": "15.8.5",
    "@nrwl/nx-cloud": "15.1.1",
    "@testcontainers/postgresql": "10.2.1",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.191",
    "@types/node": "16.18.11",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "cobertura-merge": "1.0.4",
    "cypress": "^12.17.1",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.4.3",
    "jest-junit": "15.0.0",
    "junit-report-merger": "5.0.0",
    "nx": "15.8.5",
    "prettier": "2.8.4",
    "redis-memory-server": "0.7.0",
    "rimraf": "3.0.2",
    "sequelize-cli": "6.6.0",
    "sonarqube-scanner": "3.0.1",
    "testcontainers": "10.2.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "tsx": "4.10.5",
    "reflect-metadata": "0.2.2"
  }
}
